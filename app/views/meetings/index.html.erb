


<div class="row">
  <div class="col-sm-6">
    <h1>Meetings</h1>
  </div>

  <div class="col-sm-6 text-right">
  <%= link_to new_meeting_path, class: 'btn btn-primary' do %>
    Add New Meeting
  <% end %>
  </div>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
    
        <th>Name</th>
    

            <th>Start time</th>
            <th>End time</th>
            <th>User</th>
            
      </tr>
    </thead>

    <tbody>
      <% @meetings.each do |meeting| %>
        <%= content_tag :tr, id: dom_id(meeting), class: dom_class(meeting) do %>
          
            <td><%= link_to meeting.name, meeting %></td>
          

                      <td><%= meeting.start_time %></td>
                      <td><%= meeting.end_time %></td>
                      <td><%= meeting.user %></td>
          
          
        <% end %>
      <% end %>
    </tbody>
  </table>
</div>
